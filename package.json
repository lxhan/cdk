{
	"name": "cdk",
	"version": "0.1.0",
	"bin": {
		"cdk": "bin/cdk.js"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"cdk": "cdk",
		"diff": "cdk diff --profile=$AWS_PROFILE",
		"bootstrap": "cdk bootstrap --profile=$AWS_PROFILE",
		"deploy": "cdk deploy --profile=$AWS_PROFILE",
		"destroy": "cdk destroy --profile=$AWS_PROFILE",
		"check": "biome check --write"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@types/node": "^22.5.1",
		"aws-cdk": "2.155.0",
		"dotenv": "^16.4.5",
		"tsconfig-paths": "^4.2.0",
		"tsx": "^4.19.0",
		"typescript": "~5.5.4"
	},
	"dependencies": {
		"aws-cdk-lib": "2.155.0",
		"constructs": "^10.3.0"
	}
}
